        "xmemory": "cpp",
        "xstring": "cpp",
        "xtr1common": "cpp",
        "xutility": "cpp",
        "bitset": "cpp",
        "__bit_reference": "cpp",
        "__locale": "cpp",
        "__node_handle": "cpp",
        "__split_buffer": "cpp",
        "__threading_support": "cpp",
        "__tree": "cpp",
        "__verbose_abort": "cpp",
        "print": "cpp",
        "csignal": "cpp",
        "__hash_table": "cpp",
        "queue": "cpp",
        "stack": "cpp",
        "unordered_set": "cpp",
        "__availability": "cpp",
        "future": "cpp"
